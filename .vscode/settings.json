// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "src/**/*.js.map": true,
        "src/**/*.js": true,
        "test.js.map": true,
        "test.js": true,
        ".rpt2_cache":true
    },
    "git.ignoreLimitWarning": true
}